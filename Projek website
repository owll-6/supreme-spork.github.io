<script>
    const commentsDiv = document.getElementById('comments');
    const allCommentsDiv = document.getElementById('all-comments');
    const oldestCommentsDiv = document.getElementById('oldest-comments-list');

    let allComments = [
        { name: 'Andi', phone: '08210934****', comment: 'Websitenya keren bang! 😊', rating: 5, date: new Date('2024-10-01'), replies: [] },
        { name: 'Rina', phone: '08211856****', comment: 'Sangat bermanfaat!', rating: 4, date: new Date('2024-09-21'), replies: [] }
    ];

    let oldestComments = [
        { name: 'Budi', phone: '08123456****', comment: 'Saya sangat puas dengan layanannya!', rating: 5, date: new Date('2023-12-15'), replies: [] },
        { name: 'Siti', phone: '08561234****', comment: 'Pengalaman yang luar biasa!', rating: 4, date: new Date('2023-11-10'), replies: [] }
    ];

    // Tambah komentar baru
    document.getElementById('submit-comment').addEventListener('click', () => {
        const name = document.getElementById('name').value;
        const phone = document.getElementById('phone').value;
        const commentText = document.getElementById('comment').value;
        const rating = document.getElementById('rating').value;

        if (name && phone && commentText) {
            const newComment = {
                name,
                phone: phone.replace(phone.slice(-4), '****'), // Sembunyikan 4 digit terakhir
                comment: commentText,
                rating: parseInt(rating),
                date: new Date(),
                replies: []
            };

            allComments.push(newComment);
            updateComments();

            // Kirim notifikasi ke admin via WhatsApp
            const waMessage = `Komentar baru dari ${name} (${phone.slice(0, -4)}****):\nKomentar: ${commentText}\nRating: ${rating}`;
            const waNumber = '6282328597909'; // Nomor admin WhatsApp
            const waLink = `https://api.whatsapp.com/send?phone=${waNumber}&text=${encodeURIComponent(waMessage)}`;
            window.open(waLink, '_blank');
        } else {
            alert('Harap lengkapi semua field.');
        }
    });

    // Update komentar
    function updateComments() {
        commentsDiv.innerHTML = '';
        const recentComments = allComments.slice(-5);
        recentComments.forEach(comment => {
            const commentDiv = createCommentDiv(comment);
            commentsDiv.appendChild(commentDiv);
        });
    }

    // Update komentar terlama
    function updateOldestComments() {
        oldestCommentsDiv.innerHTML = '';
        oldestComments.forEach(comment => {
            const commentDiv = createCommentDiv(comment);
            oldestCommentsDiv.appendChild(commentDiv);
        });
    }

    // Fungsi untuk membuat elemen komentar
    function createCommentDiv(comment) {
        const commentDiv = document.createElement('div');
        commentDiv.classList.add('comment');
        commentDiv.innerHTML = `
            <div class="comment-header">
                <span class="comment-user">${comment.name}</span>
                <span class="comment-date">${comment.date.toLocaleDateString()}</span>
            </div>
            <p>${comment.comment}</p>
            <p><i class="fas fa-star"></i> Rating: ${comment.rating}</p>
            <button class="reply-button">Balas sebagai admin</button>
            <div class="reply-form">
                <input type="text" class="reply-input" placeholder="Tulis balasan...">
                <button class="reply-submit">Kirim</button>
            </div>
        `;

        const replyButton = commentDiv.querySelector('.reply-button');
        const replyForm = commentDiv.querySelector('.reply-form');
        const replyInput = commentDiv.querySelector('.reply-input');
        const replySubmit = commentDiv.querySelector('.reply-submit');

        replyButton.addEventListener('click', () => {
            replyForm.style.display = 'block';
        });

        replySubmit.addEventListener('click', () => {
            const reply = replyInput.value;
            if (reply) {
                const waMessage = `Balasan admin untuk komentar dari ${comment.name} (${comment.phone.slice(0, -4)}****):\nKomentar: ${comment.comment}\nBalasan: ${reply}`;
                const waNumber = '6282328597909'; // Nomor admin WhatsApp
                const waLink = `https://api.whatsapp.com/send?phone=${waNumber}&text=${encodeURIComponent(waMessage)}`;
                window.open(waLink, '_blank');
            }
        });

        return commentDiv;
    }

    // Tampilkan/sesembunyikan komentar lama
    document.getElementById('toggle-old-comments').addEventListener('click', () => {
        const oldCommentsSection = document.getElementById('old-comments');
        const oldestCommentsSection = document.getElementById('oldest-comments');
        if (oldCommentsSection.style.display === 'none') {
            oldCommentsSection.style.display = 'block';
            oldestCommentsSection.style.display = 'block';
            document.getElementById('toggle-old-comments').innerHTML = '<i class="fas fa-chevron-up"></i> Sembunyikan Komentar Lama';
        } else {
            oldCommentsSection.style.display = 'none';
            oldestCommentsSection.style.display = 'none';
            document.getElementById('toggle-old-comments').innerHTML = '<i class="fas fa-chevron-down"></i> Tampilkan Komentar Lama';
        }
    });

    // Pemilihan bintang
    document.querySelectorAll('.star').forEach(star => {
        star.addEventListener('click', (e) => {
            document.getElementById('rating').value = e.target.getAttribute('data-value');
            document.querySelectorAll('.star').forEach(s => s.classList.remove('selected'));
            e.target.classList.add('selected');
        });
    });

    // Load awal komentar
    updateComments();
    updateOldestComments();
</script>



  <script>
    let currentSlide = 0;
    const slides = document.querySelectorAll('.slide');
    const totalSlides = slides.length;

    function showSlide(index) {
        slides.forEach((slide, i) => {
            slide.style.opacity = i === index ? 1 : 0;
        });
    }

    function nextSlide() {
        currentSlide = (currentSlide + 1) % totalSlides;
        showSlide(currentSlide);
    }

    showSlide(currentSlide);
    setInterval(nextSlide, 2000); // Ganti gambar setiap 3 detik
</script>
 <script src='https://unpkg.com/aos@2.3.0/dist/aos.js'></script><script  src="./script.js"></script>
<script>
  AOS.init({
    duration: 900,
  })
   <script>
   document.addEventListener('DOMContentLoaded', () => {
    const acc = document.querySelectorAll('.accordion');
    const toggleAllButton = document.getElementById('toggleAll');

    // Toggle individual accordion
    acc.forEach((item, index) => {
        item.addEventListener('click', () => {
            // Close all other accordions
            acc.forEach((otherItem, otherIndex) => {
                if (otherIndex !== index) {
                    otherItem.classList.remove('active');
                    const panel = otherItem.nextElementSibling;
                    panel.style.display = 'none';
                }
            });

            // Toggle clicked accordion
            item.classList.toggle('active');
            const panel = item.nextElementSibling;
            panel.style.display = panel.style.display === 'block' ? 'none' : 'block';
        });
    });

    // Toggle all accordions
    toggleAllButton.addEventListener('click', () => {
        const allOpen = Array.from(acc).every(item => item.classList.contains('active'));
        acc.forEach((item, index) => {
            item.classList.toggle('active', !allOpen);
            const panel = item.nextElementSibling;
            panel.style.display = allOpen ? 'none' : 'block';
        });
    });
});
 </script>
   
    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const acc = document.querySelectorAll('.accordion');

            acc.forEach((item) => {
                item.addEventListener('click', () => {
                    // Close other accordions
                    acc.forEach((el) => {
                        if (el !== item) {
                            el.classList.remove('active');
                            el.nextElementSibling.style.display = 'none';
                        }
                    });

                    // Toggle the active class on the clicked accordion
                    item.classList.toggle('active');

                    // Toggle the panel display
                    const panel = item.nextElementSibling;
                    if (panel.style.display === 'block') {
                        panel.style.display = 'none';
                    } else {
                        panel.style.display = 'block';
                    }
                });
            });
        });
    </script>
    
    
